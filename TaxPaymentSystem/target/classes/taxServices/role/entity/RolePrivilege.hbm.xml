<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="taxServices.role.entity">
    <class name="RolePrivilege" table="role_privilege">
      <composite-id class="RolePrivilegeId" name="id">
          <key-many-to-one name="role">
              <column name="role_id"></column>
          </key-many-to-one>
          <key-property name="code" type="java.lang.String">
              <column name="code" length="32"></column>
          </key-property>
      </composite-id>
    </class>
</hibernate-mapping>